#!/bin/bash

echo "<li>"$HOSTNAME - Stopping samba4"</li>"
#Stop samba 4
/opt/karoshi/serversetup/distro/ubuntu/scripts/control_services/samba_stop

#Install the version of samba4 from linux-schools backports
echo "<li>"$HOSTNAME - Updating repositories"</li>"
apt-get update
echo "<li>"$HOSTNAME - Installing Samba 4 from linux-schools backports"</li>"
apt-get install samba4
if [ $? != 0 ]
then
	echo "<li>"$HOSTNAME - Samba did not install correctly"</li>"
	echo "<li>"$HOSTNAME - Aborting the installation and restarting th current samba4 service"</li>"
	service samba4 start
	exit
fi

#Uninstall compiled samba4

echo "<li>"$HOSTNAME - Removing compiled samba4"</li>" 

for file in `echo "
/usr/local/lib/samba/libreplace.so
