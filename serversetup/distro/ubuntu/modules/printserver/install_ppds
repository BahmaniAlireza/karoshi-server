#!/bin/bash
echo '<pre style="font-size: 10pt; font-family:Arial, Times, Georgia, serif">'
echo "<ul><li>"Updating repository"</li></ul>"
apt-get update
echo "<ul><li>"Installing hpijs ppd files"</li></ul>"
apt-get install -y openprinting-ppd
apt-get install -y hpijs-ppds
apt-get install -y printer-driver-hpijs
apt-get install -y hplip-data
apt-get install -y printer-driver-foo2zjs
echo "</pre><br>"

